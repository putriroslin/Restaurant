{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
		"eslint:recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
		"jasmine",
		"codecept"
    ],
    "rules": {
		"no-console": 0,
		"import/no-extraneous-dependencies": 0,
		"no-underscore-dangle": 0,
		"no-restricted-globals": 0,
		"linebreak-style": 0
    }
}
